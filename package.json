  "scripts": {
    "start": "node index",
    "lint": "prettier ./src/**/* --write && eslint ./src/**/* --fix",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js --open",
    "test:ci": "yarn test --coverage",
    "test:commit": "yarn test:silent --bail --findRelatedTests",
    "test:silent": "jest --passWithNoTests --silent --noStackTrace --no-cache",
    "test:unit": "yarn test:silent -c jest-unit.config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles --verbose --no-cache",
    "prepare": "husky install"
  },
