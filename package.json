{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Christian Gama <christiangama.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && webpack --config webpack.prod.js",
    "start": "yarn codegen && webpack-dev-server --config webpack.dev.js --open",
    "prepare": "husky install",
    "reinstall": "rimraf node_modules && yarn",
    "test:ci": "yarn test:silent -c jest-ci.config.js --coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "test:commit": "yarn test:silent --bail --findRelatedTests",
    "test:silent": "jest --passWithNoTests --silent --noStackTrace --no-cache",
    "test:unit": "yarn test:silent -c jest-unit.config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles --verbose --no-cache",
    "test:integration": "yarn test:silent -c jest-integration.config.js",
    "docker:prod:run": "docker run -p 8080:80 christiangama/aicrag_frontend",
    "docker:prod": "docker buildx build -f Dockerfile.prod -t christiangama/aicrag_frontend .",
    "docker:dev:run": "docker run -p 3000:3000 christiangama/aicrag_frontend_development",
    "docker:dev": "docker buildx build -f Dockerfile.dev -t christiangama/aicrag_frontend_development .",
    "docker:down": "docker-compose down",
    "testing": "yarn test:ci",
    "docker:up": "docker-compose up --build -d",
    "docker:restart": "docker-compose restart",
    "cypress:open": "cypress open",
    "lint": "yarn format:prettier && yarn format:lint && yarn typecheck",
    "format:lint": "eslint ./src --fix",
    "format:prettier": "prettier ./src --write",
    "typecheck": "tsc -p ./tsconfig.json",
    "codegen": "rimraf ./src/infra/graphql/generated && graphql-codegen"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.3",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/luxon": "^2.0.8",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-loader": "^8.2.3",
    "coveralls": "^3.1.1",
    "cypress": "^9.2.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.27.1",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@reduxjs/toolkit": "^1.7.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.127",
    "@vanilla-extract/babel-plugin": "^1.1.3",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/recipes": "^0.2.3",
    "@vanilla-extract/sprinkles": "^1.3.3",
    "@vanilla-extract/webpack-plugin": "^2.1.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
