{
  "name": "frontend",
  "version": "1.1.0",
  "main": "index.js",
  "author": "Christian Gama <christiangama.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && vite build",
    "codegen": "rimraf ./src/infra/graphql/generated && graphql-codegen",
    "coveralls": "coveralls < coverage/lcov.info",
    "cypress:open": "cypress open",
    "dev": "vite",
    "docker:dev:run": "docker run -p 3000:3000 christiangama/aicrag_frontend_development",
    "docker:dev": "docker buildx build -f Dockerfile.dev -t christiangama/aicrag_frontend_development .",
    "docker:down": "docker-compose down",
    "docker:prod:run": "docker run -p 8080:80 christiangama/aicrag_frontend",
    "docker:prod": "docker buildx build -f Dockerfile.prod -t christiangama/aicrag_frontend .",
    "docker:restart": "docker-compose restart",
    "docker:up": "docker-compose up --build -d",
    "format:lint": "eslint . --fix",
    "format:prettier": "prettier . --write",
    "lint": "yarn format:prettier && yarn format:lint && yarn typecheck",
    "prepare": "husky install",
    "preview": "vite preview",
    "reinstall": "rimraf node_modules && yarn",
    "test:ci": "yarn test:silent -c jest-ci.config.js --coverage",
    "test:commit": "yarn test:silent --bail --findRelatedTests",
    "test:integration": "yarn test:silent -c jest-integration.config.js",
    "test:silent": "jest --passWithNoTests --silent --noStackTrace --no-cache",
    "test:unit": "yarn test:silent -c jest-unit.config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles --verbose --no-cache",
    "typecheck": "tsc -p ./tsconfig.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.3",
    "@graphql-codegen/typescript": "^2.4.2",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/luxon": "^2.0.8",
    "@types/node": "^17.0.0",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "coveralls": "^3.1.1",
    "cypress": "^9.2.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint": "^7.12.1",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@reduxjs/toolkit": "^1.7.1",
    "@vanilla-extract/babel-plugin": "^1.1.3",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/recipes": "^0.2.3",
    "@vanilla-extract/sprinkles": "^1.3.3",
    "@vanilla-extract/vite-plugin": "^3.1.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react": "^1.0.7",
    "graphql": "^16.2.0",
    "luxon": "^2.2.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react": "^17.0.2",
    "redux": "^4.1.2",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
