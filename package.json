{
  "name": "frontend",
  "version": "1.3.0",
  "main": "index.js",
  "author": "Christian Gama <christiangama.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "rimraf build && pnpm run codegen && vite build",
    "codegen": "rimraf ./src/infra/graphql/generated && graphql-codegen",
    "compose:down": "docker-compose down",
    "compose:restart": "docker-compose restart",
    "compose:up": "docker-compose up --build -d",
    "coveralls": "coveralls < coverage/lcov.info",
    "dev": "pnpm run codegen && vite --host",
    "docker:build:dev": "docker buildx build -f Dockerfile.dev -t christiangama/aicrag_frontend_development .",
    "docker:build:prod": "docker buildx build -f Dockerfile.prod -t christiangama/aicrag_frontend .",
    "docker:run:dev": "docker run -p 3000:3000 christiangama/aicrag_frontend_development",
    "docker:run:prod": "concurrently \"http-server -p 8080\" \"docker run -p 8080:80 christiangama/aicrag_frontend\"",
    "format:lint": "eslint . --fix",
    "format:prettier": "prettier . --write",
    "lint": "pnpm run format:prettier && pnpm run format:lint && pnpm run typecheck",
    "prepare": "husky install",
    "preview": "vite preview",
    "reinstall": "rimraf node_modules && pnpm run",
    "storybook": "start-storybook -p 6006",
    "test:ci": "pnpm run test:silent -- -c jest-ci.config.js --coverage",
    "test:commit": "pnpm run test:silent -- --bail --findRelatedTests",
    "test:integration": "pnpm run test:silent -- -c jest-integration.config.js",
    "test:silent": "jest --passWithNoTests --silent --noStackTrace",
    "test:unit": "pnpm run test:silent -- -c jest-unit.config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles --verbose",
    "typecheck": "tsc -p ./tsconfig.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@storybook/addon-actions": "^6.5.0-alpha.24",
    "@storybook/addon-docs": "^6.4.15",
    "@storybook/addon-essentials": "^6.5.0-alpha.24",
    "@storybook/addon-links": "^6.5.0-alpha.24",
    "@storybook/core": "6.4.13",
    "@storybook/react": "^6.5.0-alpha.24",
    "@storybook/testing-library": "^0.0.8",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "4.0.0",
    "babel-loader": "^8.2.3",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.6",
    "faker": "^5.5.3",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "storybook-builder-vite": "^0.1.13"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.4",
    "@reduxjs/toolkit": "^1.7.1",
    "@vanilla-extract/babel-plugin": "^1.1.4",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/recipes": "^0.2.3",
    "@vanilla-extract/vite-plugin": "^3.1.2",
    "@vitejs/plugin-react": "^1.1.4",
    "concurrently": "^7.0.0",
    "graphql": "^15.8.0",
    "husky": "^7.0.4",
    "luxon": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-rewrite-all": "^0.1.2"
  }
}
